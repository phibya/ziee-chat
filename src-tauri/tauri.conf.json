{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "ziee",
  "version": "0.1.0",
  "identifier": "com.ziee.ai",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist/ui"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "../hub": "hub",
      "../dist/ui": "ui",
      "./target/pdfium/target/*": "lib",
      "./target/pandoc/*": "bin",
      "./target/mistralrs/bin/*": "bin",
      "./target/llamacpp/bin/*": "bin",
      "./target/pgvector/*": "pgvector",
      "./target/pgvector/sql/*": "pgvector/sql",
      "./target/apache-age/*": "apache-age",
      "./target/apache-age/sql/*": "apache-age/sql"
    }
  }
}
